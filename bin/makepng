#!/bin/sh
#
# Convert .svg to .png
#

COMMENT="Icon by Font Awesome: https://fontawesome.com/license/free
Conversion: github.com/dharple/organizer/blob/master/bin/makepng"

convert \
	-density 13 \
	-comment "$COMMENT" \
	public/assets/fontawesome/dolly-solid-multicolor.svg \
	public/assets/fontawesome/dolly-solid-multicolor.png
