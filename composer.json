{
    "type": "project",
    "license": "BSD-2-Clause",
    "require": {
        "php": ">=8.1.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "doctrine/annotations": "^1.14.3",
        "doctrine/collections": "^1.8",
        "doctrine/doctrine-bundle": "^2.7.2",
        "doctrine/doctrine-migrations-bundle": "^2.2.3",
        "doctrine/migrations": "^2.3.5",
        "doctrine/orm": "^2.19.5",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpoffice/phpspreadsheet": "^2.2.1",
        "stof/doctrine-extensions-bundle": "^1.10",
        "symfony/asset": "^5.4.39",
        "symfony/console": "^5.4.39",
        "symfony/dotenv": "^5.4.39",
        "symfony/expression-language": "^5.4.39",
        "symfony/flex": "^1.21.6",
        "symfony/form": "^5.4.39",
        "symfony/framework-bundle": "^5.4.39",
        "symfony/intl": "^5.4.39",
        "symfony/mailer": "^5.4.39",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^5.4.39",
        "symfony/process": "^5.4.39",
        "symfony/property-access": "^5.4.39",
        "symfony/property-info": "^5.4.39",
        "symfony/proxy-manager-bridge": "^5.4.39",
        "symfony/rate-limiter": "5.4.*",
        "symfony/security-bundle": "^5.4.39",
        "symfony/serializer": "^5.4.39",
        "symfony/string": "^5.4.39",
        "symfony/translation": "^5.4.39",
        "symfony/twig-bundle": "^5.4.39",
        "symfony/validator": "^5.4.39",
        "symfony/web-link": "^5.4.39",
        "symfony/yaml": "^5.4.39",
        "twig/extra-bundle": "^2.12|^3.9.3",
        "twig/twig": "^2.12|^3.9.3"
    },
    "require-dev": {
        "outsanity/phpcs": ">=0.4.0",
        "phpstan/phpstan": "^1.10.67",
        "phpunit/phpunit": "^9.6.19",
        "rector/rector": "^0.16.0",
        "squizlabs/php_codesniffer": "^3.9.2",
        "symfony/browser-kit": "^5.4.39",
        "symfony/css-selector": "^5.4.39",
        "symfony/debug-bundle": "^5.4.39",
        "symfony/maker-bundle": "^1.50",
        "symfony/phpunit-bridge": "^5.4.39",
        "symfony/stopwatch": "^5.4.39",
        "symfony/web-profiler-bundle": "^5.4.39"
    },
    "config": {
        "platform": {
            "php": "8.1.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true
        },
        "audit": {
            "abandoned": "fail"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "go":      "php -S localhost:8000 -t public/",
        "nugo":    "symfony server:start --no-tls",
        "phpcbf":  "phpcbf",
        "phpcs":   "phpcs",
        "phpstan": "phpstan analyse",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
